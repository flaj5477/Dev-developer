<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="cacheEnabled" value="true" />
		<setting name="lazyLoadingEnabled" value="false" />
		<setting name="multipleResultSetsEnabled" value="true" />
		<setting name="useColumnLabel" value="true" />
		<setting name="useGeneratedKeys" value="false" />
		<setting name="defaultExecutorType" value="SIMPLE" />
		<setting name="defaultStatementTimeout" value="25000" />
		<!-- 오라클 컬럼이 first_name 를 VO firstName 처럼 카멜케이스로 변환 -->
		<setting name="mapUnderscoreToCamelCase" value="true" />	
	</settings>
	                       
	<!-- Alias 설정 -->
	<typeAliases>
		<!-- 화정 :이 파일을 고칠때는 pull,고침,바로commit,push 다 하기!!!! -->
		<typeAlias alias="board"
			type="com.dd.devdeveloper.board.BoardVO" />
		  
		<typeAlias alias="projects"
			type="com.dd.devdeveloper.projects.ProjectsVO" />

	
		<!-- 동우 :이 파일을 고칠때는 pull,고침,바로commit,push 다 하기!!!! -->
		<typeAlias alias="wiki"
			type="com.dd.devdeveloper.wiki.WikiVO"/>
			
		<!-- 동규 :이 파일을 고칠때는 pull,고침,바로commit,push 다 하기!!!! -->
		<typeAlias alias="testsRecord"
				   type="com.dd.devdeveloper.cbt.TestsRecordVO"/>
		<typeAlias alias="tests"
				   type="com.dd.devdeveloper.cbt.TestsVO"/>		   
		<typeAlias alias="testsQ"
				   type="com.dd.devdeveloper.cbt.TestsQVO"/>
		<typeAlias alias="testsSet"
				   type="com.dd.devdeveloper.cbt.TestsSetVO"/>	   
		<!-- 지민 :이 파일을 고칠때는 pull,고침,바로commit,push 다 하기!!!! -->
		
		
		<!-- 경민 :이 파일을 고칠때는 pull,고침,바로commit,push 다 하기!!!! -->
		<typeAlias alias="files"
			type="com.dd.devdeveloper.files.FilesVO"/>
		
		
		
		
	</typeAliases>

</configuration>